{
  "name": "nowaste-server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --max-warnings=0",
    "db:migrate": "sequelize-cli db:migrate",
